{
  "aiAgent": {
    "name": "AI Development Agent",
    "version": "1.0.0",
    "capabilities": [
      "feature-development",
      "bug-fixing", 
      "code-refactoring",
      "testing",
      "documentation",
      "ui-improvements"
    ],
    "settings": {
      "autoAssign": true,
      "autoClose": false,
      "requiresReview": true,
      "maxFilesPerTask": 10,
      "timeoutMinutes": 30
    },
    "labels": {
      "trigger": ["ai-agent", "task"],
      "progress": ["in-progress", "ai-working"],
      "completion": ["implemented", "ready-for-review", "ai-completed"]
    },
    "templates": {
      "branchPrefix": "ai-implementation-",
      "commitMessage": "ðŸ¤– AI Implementation for #{issueNumber}: {title}",
      "prTitle": "ðŸ¤– AI Implementation: {title}"
    }
  },
  "integrations": {
    "github": {
      "createPR": true,
      "addLabels": true,
      "commentOnIssues": true
    },
    "ci": {
      "runTests": true,
      "buildCheck": true,
      "lintCheck": true
    },
    "notifications": {
      "slack": false,
      "email": false,
      "discord": false
    }
  },
  "codeGeneration": {
    "framework": "next.js",
    "language": "typescript",
    "styleGuide": "prettier",
    "testFramework": "jest",
    "uiLibrary": "tailwindcss"
  }
}
